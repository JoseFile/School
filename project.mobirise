checked>\n        <input type=\"checkbox\" title=\"Show Price\" name=\"showPrice\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show List String\" name=\"showListString\" checked>\n        <input type=\"range\" inline title=\"String Count\" name=\"listGrow\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"showListString\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" condition=\"showListString\" checked>\n        <input type=\"color\" title=\"Card 1 Counter Color\" name=\"Card1CounterColor\" value=\"#bbc3cc\" condition=\" showIcons && showListString\">\n        <input type=\"color\" title=\"Card 2 Counter Color\" name=\"Card2CounterColor\" value=\"#af9f8c\" condition=\"(cardsAmount==3 || cardsAmount==2) && showIcons && showListString\">\n        <input type=\"color\" title=\"Card 3 Counter Color\" name=\"Card3CounterColor\" value=\"#bbc3cc\" condition=\"cardsAmount==3 && showIcons && showListString\">\n        <input type=\"range\" inline title=\"Icon Size\" name=\"iconSize\" min=\"0.5\" max=\"1.5\" step=\"0.1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Underline\" name=\"showUnderline\" checked>\n        \n        <input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#d1d7dd\" condition=\"showUnderline\">\n\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n\n        <input type=\"checkbox\" title=\"Show Text After Cards\" name=\"textAfterCards\" checked>\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input 